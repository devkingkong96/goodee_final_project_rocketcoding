<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="document">
  <resultMap type="document" id="documentMap">
  	<id column="DOC_NO" property="docNo"/>
		<result column="DOC_TAG" property="docTag"/>
		<result column="DOC_TITLE" property="docTitle"/>
		<result column="DOC_CONT" property="docCont"/>
		<result column="DOC_STATCD" property="docStatcd"/>
		<result column="U_DATE" property="uDate"/>
		<result column="START_DATE" property="startDate"/>
		<result column="END_DATE" property="endDate"/>
  		<result property="empNo" column="EMP_NO"/>
	  	<result property="docTag" column="DOC_TAG"/>
  </resultMap>
</mapper>
